{
  "name": "ft-transfer-api-service",
  "version": "1.0.0",
  "description": "High-performance NEAR FT transfer API service with load balancing and key rotation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "benchmark": "ts-node benchmarks/index.ts",
    "benchmark:testnet": "ts-node benchmarks/index.ts http://localhost:3000 100 10 testnet",
    "benchmark:localnet": "ts-node benchmarks/index.ts http://localhost:3000 100 10 localnet",
    "deploy": "bash scripts/deploy.sh",
    "setup": "bash scripts/setup.sh",
    "keygen": "bash scripts/keygen.sh"
  },
  "keywords": [
    "near",
    "blockchain",
    "ft-transfer",
    "api",
    "typescript",
    "high-performance"
  ],
  "author": "NEAR FT Transfer Team",
  "license": "MIT",
  "dependencies": {
    "@near-js/api": "^1.0.0",
    "@near-js/accounts": "^1.0.0",
    "@near-js/crypto": "^1.0.0",
    "@near-js/keystores": "^1.0.0",
    "@near-js/transactions": "^1.0.0",
    "@near-js/utils": "^1.0.0",
    "@near-js/providers": "^1.0.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "axios": "^1.4.0",
    "rate-limiter-flexible": "^3.0.8",
    "express-prometheus-middleware": "^1.2.0",
    "prom-client": "^14.2.0",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "bullmq": "^4.8.0",
    "p-limit": "^4.0.0",
    "p-queue": "^7.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/uuid": "^9.0.2",
    "@types/jest": "^29.5.3",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}